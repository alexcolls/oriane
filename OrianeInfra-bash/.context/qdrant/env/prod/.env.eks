# AWS EKS Configuration for Qdrant Production
AWS_REGION=us-east-1
AWS_ACCOUNT_ID=509399609859

# KMS Key for EBS Encryption
KMS_KEY_ID=8176aa31-f8d6-4cdf-b516-b9754064b978
KMS_KEY_ALIAS=alias/qdrant-production-key
KMS_KEY_ARN=arn:aws:kms:us-east-1:509399609859:key/8176aa31-f8d6-4cdf-b516-b9754064b978

# EC2 Instances
CPU_INSTANCE_TYPES=c6g.large,c6g.xlarge,c6g.2xlarge,c6g.4xlarge,c6g.8xlarge,c7g.large,c7g.xlarge,c7g.2xlarge,c6i.large,c6i.xlarge,c6i.2xlarge
GPU_INSTANCE_TYPES=g5.xlarge,g5.2xlarge,g5.4xlarge,g5.8xlarge,g4dn.xlarge,g4dn.2xlarge,g4dn.4xlarge,g4dn.8xlarge,g5g.xlarge,g5g.2xlarge,p3.2xlarge

# S3 Bucket for Backups
S3_BACKUP_BUCKET=oriane-qdrant-backups-us-east-1
S3_BACKUP_BUCKET_ARN=arn:aws:s3:::oriane-qdrant-backups-us-east-1

# IAM Roles
EKS_CLUSTER_ROLE_NAME=QdrantEKSClusterRole
EKS_CLUSTER_ROLE_ARN=arn:aws:iam::509399609859:role/QdrantEKSClusterRole
EKS_NODEGROUP_ROLE_NAME=QdrantEKSNodeGroupRole
EKS_NODEGROUP_ROLE_ARN=arn:aws:iam::509399609859:role/QdrantEKSNodeGroupRole

# Custom Policy
EBS_ENCRYPTION_POLICY_NAME=QdrantEBSEncryptionPolicy
EBS_ENCRYPTION_POLICY_ARN=arn:aws:iam::509399609859:policy/QdrantEBSEncryptionPolicy

# Network Load Balancer Configuration
EKS_CLUSTER_NAME=qdrant-production
DOMAIN_NAME=example.com

# Load Balancer Settings
NLB_SCHEME=internet-facing
NLB_TYPE=nlb
NLB_CROSS_ZONE_ENABLED=true
NLB_CONNECTION_IDLE_TIMEOUT=60
NLB_SSL_POLICY=ELBSecurityPolicy-TLS-1-2-2017-01

# Health Check Configuration
HEALTH_CHECK_PROTOCOL=HTTP
HEALTH_CHECK_PORT=6333
HEALTH_CHECK_PATH=/
HEALTH_CHECK_HEALTHY_THRESHOLD=2
HEALTH_CHECK_UNHEALTHY_THRESHOLD=2
HEALTH_CHECK_TIMEOUT=5
HEALTH_CHECK_INTERVAL=30
